extends layout

block content

	.container.mt-5
		.center-col
			h6 Are you sure you want to delete this quote?

			form(method="post")
				button.btn.btn-danger.me-3(type="submit") Yes, delete
			
				a.btn.btn-primary(href=`/quote/${quote._id}`) No, don't delete


			hr.mt-4


			+quoteCard(quote, {listPage:false})

			